{
  "name": "nodejs-fastify-ddd",
  "version": "1.0.0",
  "description": "Chassi nodejs com Fastify, DDD e CQRS",
  "main": "dist/src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\"",
    "up": "docker-compose up -d",
    "down": "docker-compose down && docker system prune -af",
    "restart": "docker-compose down && docker system prune -af && docker-compose up -d",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "clean": "rimraf dist",
    "build": "npm run clean && tsc",
    "start1": "node  -r module-alias/register -r dotenv/config -r ts-node/register -r tsconfig-paths/register src/index.ts",
    "startvelho": "node -r dotenv/config -r ts-node/register -r tsconfig-paths/register  -r module-alias/register dist/index.js",
    "start": "npm run build && node -r module-alias/register dist/src/index.js"
  },
  "keywords": [],
  "author": "Rafael de Oliveira Silva (rafael.oliveirasylva@gmail.com)",
  "license": "ISC",
  "_moduleAliases": {
    "@": "dist"
  },
  "dependencies": {
    "@fastify/cookie": "^9.3.1",
    "@fastify/cors": "^9.0.1",
    "@fastify/formbody": "^7.4.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/request-context": "^5.1.0",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^3.0.0",
    "@opentelemetry/api": "^1.8.0",
    "@opentelemetry/exporter-collector": "^0.25.0",
    "@opentelemetry/exporter-jaeger": "^1.24.1",
    "@opentelemetry/exporter-prometheus": "^0.51.1",
    "@opentelemetry/exporter-trace-otlp-http": "^0.51.1",
    "@opentelemetry/instrumentation": "^0.51.1",
    "@opentelemetry/instrumentation-amqplib": "^0.37.0",
    "@opentelemetry/instrumentation-fastify": "^0.36.1",
    "@opentelemetry/instrumentation-http": "^0.51.1",
    "@opentelemetry/instrumentation-mongodb": "^0.43.0",
    "@opentelemetry/instrumentation-pg": "^0.41.0",
    "@opentelemetry/instrumentation-pino": "^0.39.0",
    "@opentelemetry/instrumentation-redis": "^0.39.1",
    "@opentelemetry/resources": "^1.24.1",
    "@opentelemetry/sdk-node": "^0.51.1",
    "@opentelemetry/semantic-conventions": "^1.24.1",
    "@opentelemetry/tracing": "^0.24.0",
    "amqplib": "^0.10.4",
    "fastify": "^4.27.0",
    "http": "^0.0.1-security",
    "memcached": "^2.2.2",
    "module-alias": "^2.2.3",
    "mongodb": "^6.6.2",
    "pg": "^8.11.5",
    "prom-client": "^15.1.2",
    "redis": "^4.6.14",
    "yaml": "^2.4.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.5",
    "@types/memcached": "^2.2.10",
    "@types/module-alias": "^2.0.4",
    "@types/pg": "^8.11.6",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "pino-pretty": "^11.1.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.7",
    "typescript": "^5.4.5"
  }
}
